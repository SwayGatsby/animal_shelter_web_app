  <% @animals.each do |animal| %>
  <div class="container">
    <img class="animal_pic" src="<%= animal.photo %>">
    <p>Name: <%= animal.name %></p>
    <p>Type: <%= animal.type %></p>
    <p>Breed: <%= animal.breed %></p>
    <p>Admission Date: <%= animal.admission_date.to_s() %></p>
    <p>Ready for Adoption: <%= animal.is_adoptable?() %></p>
    <p><a href="/animals/<%= animal.id %>/edit">Edit Details</a></p>
    </div>
  <% end %>
